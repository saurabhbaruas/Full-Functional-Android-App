<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.Toolbar
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#dc3838"
        android:id="@+id/toolbarcontact"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:popupTheme="@style/AppTheme.PopupOverlay"
        app:title="About Us"
        />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >

            <LinearLayout
                android:id="@+id/lbl0"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:padding="10dp"
                android:background="#eeeeee">
                <TextView
                    android:id="@+id/txt0"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="ABOUT ORGANIZATION"
                    android:textSize="14sp"
                    android:textColor="#a1a1a1"
                    android:gravity="left|center"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/setting_LineColor"></View>
            <LinearLayout
                android:id="@+id/lbl00"
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:padding="10dp"
                android:clickable="true"
                android:background="@drawable/textview_effect">
                <ImageView
                    android:id="@+id/img10"
                    android:layout_width="90dp"
                    android:layout_height="match_parent"
                    android:src="@drawable/splash"
                    android:tint="#00000000"
                    android:layout_gravity="left|center"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="left|center">
                    <TextView
                        android:id="@+id/txt20"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/contactus1"
                        android:textSize="14sp"
                        android:layout_marginLeft="10dp"
                        android:textColor="#000000"/>
                    <!--<TextView-->
                    <!--android:id="@+id/txt30"-->
                    <!--android:layout_width="wrap_content"-->
                    <!--android:layout_height="wrap_content"-->
                    <!--android:layout_alignParentRight="true"-->
                    <!--android:text="3.2.3"-->
                    <!--android:textSize="14sp"-->
                    <!--android:textColor="#000000"/>-->
                </RelativeLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/setting_LineColor"></View>



            <LinearLayout
                android:id="@+id/lbl1"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:padding="10dp"
                android:background="#eeeeee">
                <TextView
                    android:id="@+id/txt1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="VERSION"
                    android:textSize="14sp"
                    android:textColor="#a1a1a1"
                    android:gravity="left|center"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/setting_LineColor"></View>
            <LinearLayout
                android:id="@+id/lbl2"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:padding="10dp"
                android:clickable="true"
                android:background="@drawable/textview_effect">
                <ImageView
                    android:id="@+id/img1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/build"
                    android:tint="#00000000"
                    android:layout_gravity="left|center"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="left|center">
                    <TextView
                        android:id="@+id/txt2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Build"
                        android:textSize="14sp"
                        android:layout_marginLeft="10dp"
                        android:textColor="#000000"/>
                    <TextView
                        android:id="@+id/txt3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="1.0"
                        android:textSize="14sp"
                        android:textColor="#000000"/>
                </RelativeLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/setting_LineColor"></View>
            <LinearLayout
                android:id="@+id/lbl3"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:padding="10dp"
                android:background="#eeeeee">
                <TextView
                    android:id="@+id/txt4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="GET HELP"
                    android:textSize="14sp"
                    android:textColor="#a1a1a1"
                    android:gravity="left|center"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/setting_LineColor"></View>
            <LinearLayout
                android:id="@+id/lbl4"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:padding="10dp"
                android:clickable="true"
                android:background="@drawable/textview_effect">
                <ImageView
                    android:id="@+id/img2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/web"
                    android:tint="#00000000"
                    android:layout_gravity="left|center"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="left|center">
                    <TextView
                        android:onClick="web"
                        android:id="@+id/txt5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Support Website"
                        android:textSize="15sp"
                        android:layout_marginLeft="10dp"
                        android:textColor="#000"/>
                </RelativeLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/setting_LineColor"></View>
            <LinearLayout
                android:id="@+id/lbl5"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:padding="10dp"
                android:clickable="true"
                android:background="@drawable/textview_effect">
                <ImageView
                    android:id="@+id/img3"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/faq"
                    android:tint="#00000000"
                    android:layout_gravity="left|center"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="left|center">
                    <TextView
                        android:id="@+id/txt6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="FAQ"
                        android:textSize="14sp"
                        android:layout_marginLeft="10dp"
                        android:textColor="#000000"/>
                </RelativeLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/setting_LineColor"></View>
            <LinearLayout
                android:id="@+id/lbl6"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:padding="10dp"
                android:background="#eeeeee">
                <TextView
                    android:id="@+id/txt7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="LEGAL"
                    android:textSize="14sp"
                    android:textColor="#a1a1a1"
                    android:gravity="left|center"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/setting_LineColor"></View>
            <LinearLayout
                android:id="@+id/lbl7"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:padding="10dp"
                android:clickable="true"
                android:background="@drawable/textview_effect">
                <ImageView
                    android:id="@+id/img4"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/ic_menu_manage"
                    android:tint="#00000000"
                    android:layout_gravity="left|center"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="left|center">
                    <TextView
                        android:id="@+id/txt8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Privacy Policy"
                        android:textSize="14sp"
                        android:layout_marginLeft="10dp"
                        android:textColor="#000000"/>
                </RelativeLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/setting_LineColor"></View>
            <LinearLayout
                android:id="@+id/lbl8"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:padding="10dp"
                android:clickable="true"
                android:background="@drawable/textview_effect">
                <ImageView
                    android:id="@+id/img5"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/ic_menu_gallery"
                    android:tint="#00000000"
                    android:layout_gravity="left|center"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="left|center">
                    <TextView
                        android:id="@+id/txt9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Terms of Service"
                        android:textSize="14sp"
                        android:layout_marginLeft="10dp"
                        android:textColor="#000000"/>
                </RelativeLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/setting_LineColor"></View>
            <LinearLayout
                android:id="@+id/lbl9"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:padding="10dp"
                android:background="#eeeeee">
                <TextView
                    android:id="@+id/txt10"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="DEVELOPED BY"
                    android:textSize="14sp"
                    android:textColor="#a1a1a1"
                    android:gravity="left|center"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/setting_LineColor"></View>
            <LinearLayout
                android:id="@+id/lbl10"
                android:layout_width="match_parent"
                android:layout_height="54dp"
                android:padding="5dp"
                android:clickable="true"
                android:background="@drawable/textview_effect">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textSize="15dp"
                    android:text="Surya Prakash Tripathi"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@drawable/splash"
                    android:tint="#00000000"
                    android:gravity="left|center"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/setting_LineColor"></View>
        </LinearLayout>
    </ScrollView>
</LinearLayout>